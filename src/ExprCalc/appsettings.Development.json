{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },

  "RestAPI": {
    "CorsAllowAny": false, // Enable CORS for all types of requests
    "UseSwagger": true // Enable SwaggerUI
  }
}
